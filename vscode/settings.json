{
    "editor.mouseWheelZoom": true,
    "editor.quickSuggestions": {
        "strings": true
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "editor.tokenColorCustomizations": {},
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.preferredDarkColorTheme": "Omni",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell",
            "args": ["-ExecutionPolicy", "Bypass"]
        }
    },
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "editor.fontLigatures": true,
    "editor.tabSize": 4,

    "workbench.colorTheme": "Aura Dark",
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontFamily": "JetBrains Mono",
    "apc.font.family": "JetBrains Mono",
    "terminal.integrated.fontFamily": "JetBrains Mono",
    "terminal.integrated.fontSize": 17,
    "editor.fontSize": 17,
    "editor.lineHeight": 1.7,
    "editor.scrollbar.verticalScrollbarSize": 7,
    "workbench.tree.indent": 25,
    "apc.activityBar": {
        "position": "bottom",
        "hideSettings": true,
        "size": 24
    },
    "apc.statusBar": {
        "position": "editor-bottom",
        "height": 22,
        "fontSize": 13
    },
    "apc.electron": {
        "titleBarStyle": "hiddenInset",
        "trafficLightPosition": {
            "x": 9,
            "y": 9
        }
    },
    "apc.header": {
        "height": 30,
        "fontSize": 14
    },
    "apc.listRow": {
        "height": 23,
        "fontSize": 13
    },
    "editor.semanticHighlighting.enabled": false,
    "workbench.list.smoothScrolling": true,
    "editor.smoothScrolling": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "workbench.colorCustomizations": {
        "editor.lineHighlightBorder": "#ffffff17"
    },
    "editor.hideCursorInOverviewRuler": true,
    "python.defaultInterpreterPath": "C:\\Users\\WLGS\\AppData\\Local\\Programs\\Python\\Python39\\python.exe",
    "editor.linkedEditing": true,
    "git.confirmSync": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "workbench.list.openMode": "doubleClick",
    "diffEditor.ignoreTrimWhitespace": false,
    "vsicons.dontShowNewVersionMessage": true,
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "explorer.confirmDragAndDrop": false,
    "css.lint.unknownAtRules": "ignore",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "[c]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.python",
        "editor.formatOnType": true
    },
    "explorer.confirmDelete": true,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.wordWrap": "on",
    "workbench.startupEditor": "none",
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[handlebars]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.cursorBlinking": true,
    "editor.minimap.enabled": false,
    "haskell.manageHLS": "PATH",
    "editor.snippetSuggestions": "bottom",
    "editor.quickSuggestionsDelay": 0,
    "editor.cursorBlinking": "smooth",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false,
        "yaml": false
    },
    "[ahk]": {
        "editor.defaultFormatter": "cweijan.vscode-autohotkey-plus"
    },
    "npm.keybindingsChangedWarningShown": true,
    "workbench.editor.empty.hint": "hidden",
    "editor.codeActionsOnSave": {},
    "editor.formatOnSave": true,
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "window.menuBarVisibility": "compact",
    "prettier.printWidth": 120,
    "prettier.tabWidth": 4,
    "editor.scrollbar.horizontalScrollbarSize": 10
}
